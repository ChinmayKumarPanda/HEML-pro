"3	LINE	0
	# ---------- builder (full Debian image for reliable native builds) ----------
	FROM node:18 AS builder
	
	# Install build tools required by native modules (node-gyp, sharp, etc.)
	# If your project definitely doesn't need native builds you can remove this RUN line.
	RUN apt-get update && apt-get install -y --no-install-recommends \
	    python3 make g++ ca-certificates \
	  && rm -rf /var/lib/apt/lists/*
	
	WORKDIR /app
	
	# Copy package files first so Docker caches npm install step when dependencies don't change
	COPY package*.json ./
	
	# Use package-lock.json if present (npm ci), otherwise npm install.
	# Also: --no-audit --prefer-offline speeds things up in CI/docker builds
	RUN if [ -f package-lock.json ]; then \
	      npm ci --only=production --no-audit --prefer-offline; \
	    else \
	      npm install --production --no-audit --prefer-offline; \
	    fi
	
	# Copy application source
	COPY . .
	
	# Optional: run build step if your app needs it, e.g. npm run build
	# RUN npm run build --if-present
	
	# ---------- runtime (slim final image) ----------
	FROM node:18-slim AS runtime
	WORKDIR /app
	
	# Copy only needed files from builder (node_modules + app)
	COPY --from=builder /app /app
	
	ENV NODE_ENV=production
	EXPOSE 3000
	
	# Use a non-root user if desired (optional)
	# RUN useradd -m app && chown -R app:app /app
	# USER app
	
	CMD ["node", "index.js"]
	
